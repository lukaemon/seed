  keywords = {formal reasoning},
  file = {/Users/lukakuma/Zotero/storage/UEFERZ6V/Deac et al. - 2021 - Neural Algorithmic Reasoners are Implicit Planners.pdf;/Users/lukakuma/Zotero/storage/XLSI3ZLQ/2110.html}
}

